{
  "name": "babel-plugin-tester",
  "version": "10.1.0",
  "description": "Utilities for testing babel plugins",
  "keywords": [
    "babel",
    "plugin",
    "tester"
  ],
  "homepage": "https://github.com/Xunnamius/babel-plugin-tester",
  "bugs": {
    "url": "https://github.com/Xunnamius/babel-plugin-tester/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Xunnamius/babel-plugin-tester"
  },
  "license": "MIT",
  "author": "Xunnamius",
  "type": "commonjs",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "setup": "npm install && npm run validate -s",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "maintained node versions"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "max-lines": 0,
      "max-lines-per-function": 0,
      "no-useless-catch": 0,
      "prefer-object-spread": 0,
      "babel/camelcase": 0,
      "babel/no-unused-expressions": 0,
      "babel/quotes": 0,
      "babel/valid-typeof": 0,
      "jest/prefer-todo": 0
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "fixtures"
  ],
  "dependencies": {
    "@types/babel-plugin-tester": "^9.0.0",
    "lodash.mergewith": "^4.6.2",
    "prettier": "^2.0.1",
    "strip-indent": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/helper-plugin-utils": "^7.19.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.10.5",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/plugin-syntax-typescript": "^7.10.4",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "kcd-scripts": "^12.3.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.11.6"
  },
  "engines": {
    "node": "^14.20.0 || ^16.16.0 || >=18.5.0"
  }
}
